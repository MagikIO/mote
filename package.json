{
  "name": "@magik_io/mote",
  "version": "1.5.1",
  "description": "A singular mote can be a speck of dust, a particle of pollen, or a tiny fragment of a mineral. In the context of this library, a mote is a small, self-contained, and reusable piece of code that can be used to build larger applications.",
  "engines": {
    "node": "21.*.*",
    "npm": "10.*.*"
  },
  "scripts": {
    "build": "vite build --mode production",
    "publish": "npm publish --access public"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "dist/Mote.cjs",
  "module": "dist/Mote.js",
  "types": "dist/Mote.d.ts",
  "exports": {
    "*": {
      "import": "./dist/Mote.js",
      "require": "./dist/Mote.cjs"
    },
    "Mote": {
      "import": "./dist/Mote.js",
      "require": "./dist/Mote.cjs"
    },
    "All": {
      "import": "./dist/All.js",
      "require": "./dist/All.cjs"
    },
    "El": {
      "import": "./dist/El.js",
      "require": "./dist/El.cjs"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/Mote.d.ts",
        "dist/El.d.ts",
        "dist/All.d.ts"
      ],
      "El": [
        "dist/El.d.ts"
      ],
      "Mote": [
        "dist/Mote.d.ts"
      ],
      "All": [
        "dist/All.d.ts"
      ]
    }
  },
  "dependencies": {
    "ulid": "^2.3.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.2.0",
    "@magik_io/lint_golem": "^2.1.1",
    "eslint": "^9.2.0",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.9.0",
    "vite": "^5.2.11",
    "vite-plugin-dts": "^3.9.1"
  },
  "authors": [
    {
      "name": "Antonio B.",
      "email": "Abourassa@AssetVal.com",
      "github": "https://github.com/Abourass"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/MagikIO/mote.git"
  },
  "browserslist": [
    "last 2 version",
    "> 1%"
  ],
  "license": "MIT",
  "packageManager": "yarn@4.2.2"
}
